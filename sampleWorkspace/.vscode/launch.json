  "version": "0.2.0",
  "configurations": [
    {
      "type": "cc65-dbg",
      "request": "launch",
      "name": "Debug file",
      "command": "../../emu/build/emu",
      "program": "hello_world.bin",
      "args": ["--dap"],
      "stopOnEntry": true,
      "cwd": "${workspaceFolder}",
      "trace": true
    },
    {
      "type": "cc65-dbg",
      "request": "attach",
      "name": "Attach running",
      "program": "hello_world.bin",
      "port": 4567,
      "trace": true
    }
  ]
}
