{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "cc65-dbg",
			"request": "launch",
			"name": "Debug file",
			"program": "${file}",
			"stopOnEntry": true,
			"cwd": "${cwd}"
		},
		{
			"type": "cc65-dbg",
			"request": "launch",
			"name": "Launch with args",
			"program": "${cwd}",
			"args": [
				{
					"sbin": "${cwd}\\hello_world.bin",
					"src": "${cwd}",
					"list": "${cwd}",
					"input": "0xf004",
					"output": "0xf001"
				}
			],
			"stopOnEntry": true,
			"cwd": "${cwd}"
		},
		{
			"type": "cc65-dbg",
			"request": "launch",
			"name": "Launch with 65C02",
			"program": "${cwd}",
			"args": [
				{
					"cpu": "65C02",
					"sbin": "${cwd}\\hello_world.bin",
					"src": "${cwd}",
					"list": "${cwd}",
					"input": "0xf004",
					"output": "0xf001"
				}
			],
			"stopOnEntry": true,
			"cwd": "${cwd}"
		}
	]
}
